{
  "expo": {
    "name": "YOARADIO",
    "slug": "replib",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/church-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/church-logo.png",
      "animationType": "fade",
      "resizeMode": "contain",
      "backgroundColor": "#050505"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.yoaradio.new",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/church-logo.png",
        "backgroundColor": "#050505"
      },
      "permissions": [
        "INTERNET",
        "AUDIO_SETTINGS",
        "WAKE_LOCK"
      ],
      "package": "app.yoaradio.com",
      "googleServicesFile": "./google-services.json",
      "versionCode": 8
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ],
      [
        "@react-native-firebase/app"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6feac33f-8453-4c48-9f45-928d2f68a456"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/5e467efd-cc10-43cf-930b-99c5b6c786c9"
    },
    "owner": "fredabila"
  }
}
